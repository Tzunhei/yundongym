{
 "presets": [
   ["@babel/preset-env", {
     "targets": {
       "node": "current"
     }
   }]
 ],
 "plugins": [
    ["module-resolver", {
      "root":  "./src",
      "alias": {
        "@utils": "./src/app/utils",
        "@modules": "./src/app/modules",
        "@models": "./src/db/models"
      }
    }]
  ]
}