{
  "name": "yundongym",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --watch",
    "dev": "nodemon -r dotenv/config ./src/server.js --exec babel-node",
    "dev:start": "concurrently --kill-others-on-fail \"yarn test\"  \"yarn dev\"",
    "build": "babel ./src -d dist",
    "serve": "node dist/server.js",
    "lint:fix": "eslint . --ext .js --cache --fix",
    "migration:delete": "sequelize-cli db:migrate:undo:all",
    "migration:run": "sequelize-cli db:migrate",
    "seed:delete": "sequelize-cli db:seed:undo:all",
    "seed:run": "sequelize-cli db:seed:all",
    "db:delete": "yarn migration:delete && yarn seed:delete",
    "db:reset": "yarn migration:delete && yarn migration:run && yarn seed:run",
    "db:update": "yarn migration:run && yarn seed:run"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.1.1",
    "apollo-server": "^2.13.0",
    "apollo-server-testing": "^2.16.0",
    "bcrypt": "^4.0.1",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.8",
    "mysql2": "^2.1.0",
    "pg": "^8.1.0",
    "sequelize": "^5.21.7",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1"
  }
}
